{
	"useTabs": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"arrowParens": "always",
	"bracketSpacing": true,
	"printWidth": 80,
	"semi": false,
	"singleQuote": true,
	"proseWrap": "always",
	"overrides": [
		{
			"files": "*.jsx",
			"options": {
				"tabWidth": 2,
				"bracketSameLine": true,
				"htmlWhitespaceSensitivity": "strict",
				"jsxSingleQuote": false,
				"singleAttributePerLine": true
			}
		}
	]
}
