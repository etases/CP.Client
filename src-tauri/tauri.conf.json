{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "",
		"beforeDevCommand": "",
		"devPath": "https://localhost:3000",
		"distDir": "../dist"
	},
	"package": {
		"productName": "CP.Client",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"http": {
				"all": true,
				"request": true,
				"scope": ["https://**", "http://**"]
			}
		},
		"bundle": {
			"active": true,
			"category": "Education",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "cp.client",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null,
			"devCsp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"center": true,
				"fullscreen": false,
				"height": 720,
				"resizable": false,
				"title": "CP.Client",
				"width": 1280
			}
		]
	}
}
